{
  "name": "inspook-client",
  "version": "1.0.0",
  "description": "Front-end React Client",
  "main": "index.js",
  "scripts": {
    "build-css": "node-sass-chokidar src/assets/sass/ -o src/assets/css/",
    "watch-css": "npm run build-css && node-sass-chokidar src/assets/sass -o src/assets/css/ --watch --recursive --importer=node_modules/node-sass-tilde-importer",
    "start": "react-scripts start",
    "build": "npm run build-css && react-scripts build"
  },
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ],
  "author": "Sebastian Pobocha",
  "license": "ISC",
  "dependencies": {
    "@babel/preset-env": "^7.0.0-rc.0",
    "amazon-product-api": "^0.4.4",
    "apollo-cache-inmemory": "^1.4.3",
    "apollo-client": "^2.4.13",
    "apollo-link": "^1.2.11",
    "apollo-link-http": "^1.5.11",
    "apollo-utilities": "^1.2.1",
    "axios": "^0.18.0",
    "babel-loader": "8.0.5",
    "babel-plugin-root-import": "^6.1.0",
    "goodreads-api-node": "0.0.10",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "http-proxy-middleware": "^0.19.1",
    "node-sass-chokidar": "^1.3.3",
    "node-sass-tilde-importer": "^1.0.2",
    "prop-types": "^15.6.2",
    "react": "^16.4.0",
    "react-apollo": "^2.4.1",
    "react-dom": "^16.4.0",
    "react-lines-ellipsis": "^0.14.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.8",
    "react-sticky-el": "^1.0.20",
    "string-strip-html": "^4.0.12",
    "swiper": "^4.4.6",
    "xml2js": "^0.4.19"
  }
}
